        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            },
            {
              "name": "Authorization",
              "value": "=Bearer {{ $env.KIE_AI_API_KEY }}"
            }
          ]
        },
        "sendBody": true,
        "contentType": "json",
        "body": "={\n  \"model\": \"nano-banana-pro\",\n  \"prompt\": \"{{ $('On form submission').item.json.body.prompt }}\",\n  \"image_url\": \"{{ $json.data.url }}\",\n  \"negative_prompt\": \"blurry, low quality, distorted\",\n  \"width\": 1024,\n  \"height\": 1024,\n  \"num_inference_steps\": 20,\n  \"guidance_scale\": 7.5\n}",
        "options": {
          "timeout": 60000
        }
      },
      "id": "edit-image",
      "name": "Edit Image",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [650, 300]
    },
    {
      "parameters": {
        "amount": 5,
        "unit": "seconds"
      },
      "id": "wait1",
      "name": "Wait1",
      "type": "n8n-nodes-base.wait",
      "typeVersion": 1,
      "position": [850, 300]
    },
    {
      "parameters": {
        "method": "GET",
