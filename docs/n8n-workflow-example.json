{
  "name": "Generate Ad with KIE.AI Nano Banana Pro",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "generate-ad",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "On form submission",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300],
      "webhookId": "generate-ad-webhook"
